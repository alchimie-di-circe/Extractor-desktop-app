{
  "name": "electron-svelte",
  "productName": "electron-svelte",
  "description": "Electron Svelte",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": ".vite/build/main.js",
  "author": {
    "name": "Pandoks_",
    "email": "35944715+Pandoks@users.noreply.github.com"
  },
  "scripts": {
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run && npm run test:e2e",
    "test:e2e": "playwright test",
    "start": "electron-forge start",
    "dev": "./scripts/dev.sh",
    "package": "vite build && electron-forge package",
    "make": "vite build && electron-forge make -- --arch=all",
    "publish": "electron-forge publish"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-dmg": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron-forge/plugin-vite": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@playwright/test": "^1.55.1",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.16.3",
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "^4.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.4",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/node": "^22",
    "electron": "^36.8.3",
    "jsdom": "^26.0.0",
    "svelte": "^5.11.0",
    "svelte-check": "^4.1.0",
    "tailwindcss": "^4.0.3",
    "task-master-ai": "^0.40.1",
    "typescript": "^5.7.3",
    "vite": "^6.4.1",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^11.0.2",
    "keytar": "^7.9.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "electron",
      "electron-winstaller",
      "keytar"
    ],
    "patchedDependencies": {
      "@sveltejs/kit": "patches/@sveltejs__kit.patch"
    },
    "overrides": {
      "cookie@<0.7.0": ">=0.7.0",
      "brace-expansion@>=1.0.0 <=1.1.11": ">=1.1.12",
      "brace-expansion@>=2.0.0 <=2.0.1": ">=2.0.2",
      "devalue@<5.3.2": ">=5.3.2",
      "vite@>=6.0.0 <=6.4.0": ">=6.4.1",
      "playwright@<1.55.1": ">=1.55.1",
      "tmp@<=0.2.3": ">=0.2.4",
      "qs@<6.14.1": ">=6.14.1"
    }
  }
}
