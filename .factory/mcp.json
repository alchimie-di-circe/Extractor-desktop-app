{
  "mcpServers": {
    "task-master-ai": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "task-master-ai"],
      "env": {
        "TASK_MASTER_TOOLS": "all"
      }
    },
    "svelte": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@sveltejs/mcp"]
    },
    "devserver-mcp": {
      "type": "sse",
      "url": "http://127.0.0.1:9338/sse"
    },
    "shadcn-svelte": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "mcp-remote", "https://shadcn-svelte.mastra.cloud/api/mcp/shadcn/mcp"]
    },
    "wallaby": {
      "type": "stdio",
      "command": "node",
      "args": ["/Users/alexandthemusic/.wallaby/mcp"]
    }
  }
}